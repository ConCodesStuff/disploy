    "ink": "^3.2.0",
    "ink-select-input": "^4.2.1",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^4.0.3",
    "ora": "^6.1.2",
    "react": "^18.2.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@meetuplol/typewriter": "^1.0.1",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.21",
    "@types/yargs": "^17.0.11",
    "esbuild": "^0.15.10",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-esbuild": "^4.10.1",
    "tsconfig": "*",
    "typescript": "^4.5.3"
