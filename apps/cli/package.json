{
  "name": "disploy",
  "version": "0.1.0",
  "main": "dist/disploy.js",
  "bin": "disploy",
  "scripts": {
    "build": "rollup -c && echo '#!/usr/bin/env node' | cat - dist/disploy.js > disploy && chmod +x disploy",
